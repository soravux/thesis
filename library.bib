journal = {Int. J. Robot. Res.},
keywords = {000{\_}VeryGood,FAB-M,PlaceRecognition,Robotics,SLAM,SensorCamera,appearance-based navigation,fab-map,robotics,slam},
mendeley-tags = {000{\_}VeryGood,PlaceRecognition,SLAM,SensorCamera},
