@article{zhang-iccv-03,
abstract = {We present an algorithm for computing optical flow, shape, motion,$\backslash$nlighting, and albedo from an image sequence of a rigidly-moving Lambertian$\backslash$nobject under distant illumination. The problem is formulated in a$\backslash$nmanner that subsumes structure from motion, multiview stereo, and$\backslash$nphotometric stereo as special cases. The algorithm utilizes both$\backslash$nspatial and temporal intensity variation as cues: the former constrains$\backslash$nflow and the latter constrains surface orientation; combining both$\backslash$ncues enables dense reconstruction of both textured and textureless$\backslash$nsurfaces. The algorithm works by iteratively estimating affine camera$\backslash$nparameters, illumination, shape, and albedo in an alternating fashion.$\backslash$nResults are demonstrated on videos of hand-held objects moving in$\backslash$nfront of a fixed light and camera.},
