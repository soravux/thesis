isbn = {0162-8828},
issn = {0162-8828},
journal = {IEEE Trans. Pattern Anal. Mach. Intell.},
keywords = {Index Terms- Energy minimization,graph algorithms,image restoration,maximum flow,minimum cut,multicamera scene reconstruction.,segmentation,stereo},
number = {9},
pages = {1124--1137},
pmid = {15742889},
primaryClass = {cs},
title = {{An experimental comparison of min-cut/max- flow algorithms for energy minimization in vision}},
